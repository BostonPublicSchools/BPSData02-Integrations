                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  DISTINCT
C.TRS_ID AS BPSID ,
C.TRS_FIELDB_001 AS SerialNumber ,
ISNULL(( SELECT TOP 1
                R.RCD_CODE_STATE
            FROM   aspendb.x2data.dbo.STUDENT_TRANSPORTATION_SESSION B
                INNER JOIN aspendb.x2data.dbo.REF_CODE R ON R.RCD_CODE = B.STS_FIELDB_007
                                            AND C.TRS_OID = B.STS_TRS_OID
        ), '') AS DeactivateCode
FROM aspendb.x2data.dbo.TRANSPORTATION_RESOURCE C
WHERE C.TRS_FIELDA_002 = 1 --'Pass deactivated'
AND ISNULL(C.TRS_FIELDB_005, '') = '' --DEACTIVATION SENT DATE
AND C.TRS_TYPE = 'PASS'
AND ISNULL(C.TRS_FIELDB_001,'') &lt;&gt; '' --SerialNumber
AND C.TRS_FIELDB_002 = 'BPS';</property>
