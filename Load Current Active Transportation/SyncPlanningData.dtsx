       CASE
           WHEN InTrip IS NOT NULL THEN
               CAST(StopTime AS TIME)
           ELSE
               NULL
       END InStopTime,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransX
           ELSE
               X
       END inX,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransY
           ELSE
               Y
       END inY,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransLatitude
           ELSE
               Latitude
       END inLatitude,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransLongitude
           ELSE
               Longitude
       END inLongitude,
       BusID OutBusID,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               StopDescription
           ELSE
               schname_23
       END OutStopDescription,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               CAST(StopTime AS TIME)
           ELSE
               NULL
       END OutStopTime,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransX
           ELSE
               X
       END outX,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransY
           ELSE
               Y
       END outY,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransLatitude
           ELSE
               Latitude
       END outLatitude,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransLongitude
           ELSE
               Longitude
       END outLongitude
FROM Transportation.VersatransDataLive
LEFT JOIN dbo.schoollocation ON schoollocation.sch = VersatransDataLive.Sch
WHERE NOT ProgID IN ( 50, 61, 62, 70, 60 )
      AND VersatransDataLive.Sch IN (
                     SELECT DISTINCT
                         SKL_SCHOOL_ID COLLATE DATABASE_DEFAULT
                     FROM ASPENDB.x2data.dbo.STUDENT
                         JOIN ASPENDB.x2data.dbo.SCHOOL
                             ON SKL_OID = STD_SKL_OID
                     WHERE STD_ENROLLMENT_STATUS IN ( 'Active', 'Registered' )
  )
  ORDER BY StudentNo,Sch,steEligibility,Days,Trip</property>
