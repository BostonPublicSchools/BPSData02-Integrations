               'M'
           WHEN 32 THEN
               'M'
           WHEN 40 THEN
               'W'
           WHEN 41 THEN
               'W'
           WHEN 42 THEN
               'W'
           WHEN 50 THEN
               'T'
           WHEN 51 THEN
               'O'
