                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT StudentNo,
       VersatransDataLive.Sch,
   ProgID,
   BusID,
  StopDescription,
   StopTime,
       convert(char(10),Cast(CASE ProgID
           WHEN 10 THEN
               'C'
           WHEN 11 THEN
               'C'
           WHEN 12 THEN
               'C'
           WHEN 20 THEN
               'D'
           WHEN 21 THEN
               'D'
           WHEN 30 THEN
               'M'
