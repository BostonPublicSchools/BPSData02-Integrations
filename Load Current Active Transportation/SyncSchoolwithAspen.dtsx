                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT   s.SKL_SCHOOL_ID AS SchID,
       s.SKL_SCHOOL_NAME AS SchName,
       s.SKL_FIELDD_001 AS SchName_f,
       s.SKL_FIELDA_001 AS LowGrade,
       s.SKL_FIELDA_002 HighGrade,
       s.SKL_SCHOOL_LEVEL_CODE AS SchLevel,
       s.SKL_FIELDA_016 AS SpecialAdmInd,
       s.SKL_FIELDB_002 AS IsCityWide,
       s.SKL_FIELDA_015 AS ExamSchoolInd,
       s.SKL_FIELDB_035 AS ISEECChoice,
       s.SKL_FIELDC_007 AS Pathway,
       s.SKL_FIELDB_033 AS SchoolGroup,
       s.SKL_FIELDA_011 AS SharedSchoolID,
       NULLIF(s.SKL_FIELDA_005, '') AS Network
       ,CONVERT(VARCHAR(10),SKL_ZONE) AS Zone 
       ,s.SKL_FIELDA_019 AS ELLCluster,
       s.SKL_FIELDA_020 AS SPEDCluster,
     --  s.SKL_FIELDC_017 AS SchType
        SKL_SCHOOL_TYPE_CODE AS SchType
       ,SKL_FIELDB_031 AS UseTierForHMB--?
      ,DATEADD(s, s.SKL_LAST_MODIFIED / 1000, '1970-01-01 00:00:00') AS AspenLastUpdated,
       adrs.ADR_POSTAL_CODE AS schZipCode,
       s.SKL_FIELDC_002 AS SchHours,
       adrs.ADR_ADDRESS_LINE_01 AS schAddress,
       adrs.ADR_CITY AS schCity,
       adrs.ADR_PHONE_01 AS schPhone,
	   CASE WHEN s.SKL_INACTIVE_IND=1 then 'Inactive' else 'Active' end as schStatus
FROM ASPENDB.x2data.dbo.SCHOOL s WITH (NOLOCK)
    INNER JOIN ASPENDB.x2data.dbo.ORGANIZATION WITH (NOLOCK)
        ON s.SKL_ORG_OID_1 = ORG_OID
    INNER JOIN ASPENDB.x2data.dbo.DISTRICT_SCHOOL_YEAR_CONTEXT WITH (NOLOCK)
        ON CTX_OID = ORG_CTX_OID_CURRENT
    LEFT OUTER JOIN ASPENDB.x2data.dbo.PERSON_ADDRESS AS adrs WITH (NOLOCK)
        ON s.SKL_ADR_OID = adrs.ADR_OID
WHERE ISNUMERIC(s.SKL_SCHOOL_ID) = 1
ORDER BY SchID;</property>
