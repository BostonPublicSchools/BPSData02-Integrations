              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
std.StdId as parStdId
,PSN_OID as parParentUniqueId
,cp.PSN_NAME_FIRST as parFirstName
,cp.PSN_NAME_MIDDLE as parMiddleName
,cp.PSN_NAME_LAST as parLastSurname
,isnull(DATEADD(S, CTJ_LAST_MODIFIED/1000, '1970-01-01 00:00:00'),getdate()) as parLastModifiedDate
FROM BPSInterface.BPS.Student std (NOLOCK)
JOIN ASPENDB.x2data.dbo.STUDENT s (NOLOCK)
on std.std_localid = STD_ID_LOCAL collate database_default
--JOIN ASPENDB.x2data.dbo.PERSON p (NOLOCK)
--ON p.PSN_OID = s.STD_PSN_OID
JOIN ASPENDB.x2data.dbo.STUDENT_CONTACT sc (NOLOCK)
ON sc.CTJ_STD_OID = s.STD_OID
AND sc.CTJ_RELATIONSHIP_CODE IN ('Parent 2', 'Guardian', 'Parent/Guardian', 'Parent', 'Parent 1', 'Foster Parent','Caregiver')
JOIN ASPENDB.x2data.dbo.CONTACT c (NOLOCK)
ON c.CNT_OID = sc.CTJ_CNT_OID
JOIN ASPENDB.x2data.dbo.PERSON cp
ON cp.PSN_OID = c.CNT_PSN_OID
--join ASPENDB.x2data.[dbo].PERSON_ADDRESS ca WITH (NOLOCK) ON ca.ADR_OID = cp.PSN_ADR_OID_PHYSICAL
WHERE std.std_type in ('Active','Registered')
and cp.PSN_NAME_FIRST is not null and cp.PSN_NAME_LAST is not NULL</property>
