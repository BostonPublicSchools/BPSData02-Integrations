          SQLTask:SqlStatementSource="IF EXISTS(SELECt 1 from  Eligibility.SchoolCapacity SC &#xA;where (scaSCA_OID is null or scaSCA_OID = ' '))&#xA;BEGIN&#xA;&#xA;UPDATE   SC&#xA;SET SC.scaSCA_OID=a.SCA_OID&#xA;FROM&#xA;Eligibility.SchoolCapacity SC, (SELECT SKL_SCHOOL_ID ,&#xA;SCA_GRADE_LEVEL ,&#xA;SCA_PROGRAM_CODE ,&#xA;CTX_CONTEXT_ID ,&#xA;SCA_OID&#xA;FROM ASPENDB.x2data.dbo.SCHOOL&#xA;JOIN ASPENDB.x2data.dbo.SCHOOL_CAPACITY ON SKL_OID = SCA_SKL_OID&#xA;JOIN ASPENDB.x2data.dbo.DISTRICT_SCHOOL_YEAR_CONTEXT ON CTX_OID = SCA_CTX_OID&#xA;AND CTX_CONTEXT_ID &gt;= '2017-2018') a&#xA;where (scaSCA_OID is null or scaSCA_OID = ' ')&#xA;and scayearcontext &gt;='2017-2018'&#xA;and sc.scaschid collate SQL_Latin1_General_CP1_CI_AS=a.SKL_SCHOOL_ID&#xA;and sc.scaGradelevel collate SQL_Latin1_General_CP1_CI_AS =a.SCA_GRADE_LEVEL&#xA;and sc.scaProgramid collate SQL_Latin1_General_CP1_CI_AS =a.SCA_PROGRAM_CODE&#xA;END&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
