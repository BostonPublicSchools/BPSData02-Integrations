                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT 
		a.BusID,
            a.Days,
            a.EffectiveDate,
            a.InBusID,
           a.inLatitude,
          a.inLongitude,
        a.InStopDescription,
            a.InStopTime,
            a.inX,
            a.inY,
            a.OutBusID,
            a.outLatitude,
            a.outLongitude,
          a.OutStopDescription,
            a.OutStopTime,
            a.outX,
            a.outY,
            a.ProgID,
            a.Sch,
           a.steEligibility1,
            b.steEligibility,
            b.steId,
            b.steSchoolLocalId,
            b.steStudentLocalId,
            a.StopDescription,
            a.StopTime,
            b.strDayOfWeek,
            b.strId,
            b.strisCurrent,
            b.strRoutingdirection,
          CASE WHEN  b.strSourceData IS NULL THEN 'Live' ELSE b.strSourceData END AS strSourceData,
            b.strSourceId,
            a.StudentNo,
          a.Trip 
			FROM
			 (

SELECT StudentNo,
       VersatransDataLive.Sch,
       Null as StrSourceData,
       ProgID,
	   BusID,
	   StopDescription,
	   StopTime,
	convert(char(10),Cast(CASE ProgID
           WHEN 10 THEN
               'C'
           WHEN 11 THEN
               'C'
           WHEN 12 THEN
               'C'
           WHEN 20 THEN
               'D'
           WHEN 21 THEN
               'D'
           WHEN 30 THEN
               'M'
           WHEN 31 THEN
               'M'
           WHEN 32 THEN
               'M'
           WHEN 40 THEN
               'W'
           WHEN 41 THEN
               'W'
           WHEN 42 THEN
               'W'
           WHEN 50 THEN
               'T'
           WHEN 51 THEN
               'O'
           WHEN 80 THEN 
		       'A'
		   WHEN 81 THEN 
		       'A'
		   WHEN 82 THEN 
		       'A'
       END as varchar(10)))  steEligibility1 ,
       REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(Days, 'M', 'Mo'), 'T', 'Tu'), 'W', 'We'), 'H', 'Th'), 'F', 'Fr') Days,
       CAST(EffectiveDate AS DATE) EffectiveDate,
       CASE
           WHEN InTrip IS NOT NULL THEN
               'InBound'
           WHEN OutTrip IS NOT NULL THEN
               'OutBound'
           ELSE
               'Unknown'
       END Trip,
       BusID InBusID,
       CASE
           WHEN InTrip IS NOT NULL THEN
               StopDescription
           ELSE
               S.SchName COLLATE SQL_Latin1_General_CP1_CI_AS
       END InStopDescription,
    CASE
           WHEN InTrip IS NOT NULL THEN
               CAST(StopTime AS TIME)
           ELSE
               vdldropofftime
       END InStopTime,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransX
           ELSE
               SGI.sgiCartesianXCoordinate
       END inX,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransY
           ELSE
               SGI.sgiCartesianYCoordinate
       END inY,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransLatitude
           ELSE
               SGI.sgiLatitude
       END inLatitude,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransLongitude
           ELSE
               SGI.sgiLongitude
		END inLongitude,
       BusID OutBusID,
    CASE
           WHEN OutTrip IS NOT NULL THEN
               StopDescription
           ELSE
               S.SchName COLLATE SQL_Latin1_General_CP1_CI_AS
       END   OutStopDescription,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               CAST(StopTime AS TIME)
           ELSE
               vdldropofftime
       END OutStopTime,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransX
           ELSE
               SGI.sgiCartesianXCoordinate
       END outX,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransY
           ELSE
               sgi.sgiCartesianYCoordinate
       END outY,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransLatitude
           ELSE
               SGI.sgiLatitude
       END outLatitude,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransLongitude
           ELSE
               SGI.sgiLongitude
       END outLongitude
FROM Transportation.VersatransDataLive
--LEFT JOIN dbo.schoollocation ON schoollocation.sch = Transportation.VersatransDataLive.Sch
LEFT JOIN [BPS].[SchoolGeographicInformation] SGI ON SGI.sgiSchoolId = Transportation.VersatransDataLive.Sch
INNER JOIN BPS.School S ON S.SchID=VersatransDataLive.Sch 
WHERE NOT ProgID IN ( 50, 61, 62, 70, 60 )
      AND VersatransDataLive.Sch IN (
                     SELECT DISTINCT
                         SKL_SCHOOL_ID COLLATE DATABASE_DEFAULT
                     FROM ASPENDB.x2data.dbo.STUDENT
                         JOIN ASPENDB.x2data.dbo.SCHOOL
                             ON SKL_OID = STD_SKL_OID
                     WHERE STD_ENROLLMENT_STATUS IN ( 'Active', 'Registered' )
  ))a
   
  INNER JOIN (

  SELECT steId,
       steStudentLocalId ,
       steSchoolLocalId ,
      steEligibility,
       strId,
       strSourceId,
        strisCurrent ,
		strSourceData,
       CONVERT(VARCHAR(7),strDayOfWeek) strDayOfWeek ,
       strEffectiveDate,
       CONVERT(Varchar(8),strRoutingdirection) strRoutingdirection,
       strOriginPointVehicleNumber,
       strOriginPointBusStopDescription,
       strOriginPointArrivalTime,
       strOriginCartesianXCoordinate,
       strOriginCartesianYCoordinate,
       strOriginLatitude,
       strOriginLongitude,
       strDestinationPointVehicleNumber,
       strDestinationPointBusStopDescription,
       strDestinationPointArrivalTime,
       strDestinationCartesianXCoordinate,
       strDestinationCartesianYCoordinate,
       strDestinationLatitude,
       strDestinationLongitude
FROM Transportation.StudentTransportationEligibility
    left JOIN Transportation.StudentTransportationRouting
        ON StudentTransportationRouting.strSteId = StudentTransportationEligibility.steId AND ISNULL(StudentTransportationRouting.strSourceData,'Live')='Live'
 WHERE steAcademicYearDesignator =CASE WHEN MONTH(GETDATE()) &gt;= 1 AND MONTH(GETDATE()) &lt; 6 then CONCAT(YEAR(GETDATE())-1,'-', (YEAR(GETDATE())-1)+1)
                                 ELSE CONCAT(YEAR(GETDATE()),'-', (YEAR(GETDATE()))+1) END 
AND StudentTransportationEligibility.steisCurrent=1)b
on a.StudentNo=b.steStudentLocalId and a.sch=b.SteSchoolLocalId and a.SteEligibility1=b.SteEligibility and a.Trip=ISNULL(b.StrRoutingDirection,a.trip)</property>
