                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT * FROM 
(
SELECT StudentNo,
       VersatransDataLive.Sch,
       CASE ProgID
           WHEN 10 THEN
               'C'
           WHEN 11 THEN
               'C'
           WHEN 12 THEN
               'C'
           WHEN 20 THEN
               'D'
           WHEN 21 THEN
               'D'
           WHEN 30 THEN
               'M'
           WHEN 31 THEN
               'M'
           WHEN 32 THEN
               'M'
           WHEN 40 THEN
               'W'
           WHEN 41 THEN
               'W'
           WHEN 42 THEN
               'W'
           WHEN 50 THEN
               'T'
           WHEN 51 THEN
               'O'
       END steEligibility1,
       REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(Days, 'M', 'Mo'), 'T', 'Tu'), 'W', 'We'), 'H', 'Th'), 'F', 'Fr') Days,
       CAST(EffectiveDate AS DATE) EffectiveDate,
       CASE
           WHEN InTrip IS NOT NULL THEN
               'InBound'
           WHEN OutTrip IS NOT NULL THEN
               'OutBound'
           ELSE
               'Unknown'
       END Trip,
       BusID InBusID,
       CASE
           WHEN InTrip IS NOT NULL THEN
               StopDescription
           ELSE
               schname_23
       END InStopDescription,
       CASE
           WHEN InTrip IS NOT NULL THEN
               CAST(StopTime AS TIME)
           ELSE
               NULL
       END InStopTime,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransX
           ELSE
               X
       END inX,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransY
           ELSE
               Y
       END inY,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransLatitude
           ELSE
               Latitude
       END inLatitude,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdlVersatransLongitude
           ELSE
               Longitude
       END inLongitude,
       BusID OutBusID,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               StopDescription
           ELSE
               schname_23
       END OutStopDescription,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               CAST(StopTime AS TIME)
           ELSE
               NULL
       END OutStopTime,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransX
           ELSE
               X
       END outX,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransY
           ELSE
               Y
       END outY,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransLatitude
           ELSE
               Latitude
       END outLatitude,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdlVersatransLongitude
           ELSE
               Longitude
       END outLongitude
FROM Transportation.VersatransDataLive
LEFT JOIN dbo.schoollocation ON schoollocation.sch = VersatransDataLive.Sch
WHERE NOT ProgID IN ( 50, 61, 62, 70, 60 )
      AND VersatransDataLive.Sch IN (
                     SELECT DISTINCT
                         SKL_SCHOOL_ID COLLATE DATABASE_DEFAULT
                     FROM ASPENDB.x2data.dbo.STUDENT
                         JOIN ASPENDB.x2data.dbo.SCHOOL
                             ON SKL_OID = STD_SKL_OID
                     WHERE STD_ENROLLMENT_STATUS IN ( 'Active', 'Registered' )
  ))
  A
LEFT  JOIN 
(
SELECT steId,
       steStudentLocalId,
       steSchoolLocalId,
       steEligibility,
       strId,
       strSourceId,
       CASE WHEN strisCurrent IS NULL THEN 1 ELSE 0 END AS strisCurrent,
       strDayOfWeek,
       strEffectiveDate,
       strRoutingdirection,
       strOriginPointVehicleNumber,
       strOriginPointBusStopDescription,
       strOriginPointArrivalTime,
       strOriginCartesianXCoordinate,
       strOriginCartesianYCoordinate,
       strOriginLatitude,
       strOriginLongitude,
       strDestinationPointVehicleNumber,
       strDestinationPointBusStopDescription,
       strDestinationPointArrivalTime,
       strDestinationCartesianXCoordinate,
       strDestinationCartesianYCoordinate,
       strDestinationLatitude,
       strDestinationLongitude
FROM Transportation.StudentTransportationEligibility
 LEFT    JOIN Transportation.StudentTransportationRouting
        ON StudentTransportationRouting.strSteId = StudentTransportationEligibility.steId
WHERE NULLIF(steEligiblityEndDate, '') IS NULL)B
ON  A.StudentNo=B.steStudentLocalId 
AND A.Sch=B.steSchoolLocalId AND A.steEligibility1=B.steEligibility</property>
