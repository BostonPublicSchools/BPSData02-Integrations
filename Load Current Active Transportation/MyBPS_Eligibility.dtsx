                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT Convert(varchar(10),n.STUDENTNO) as steStudentLocalId

,getdate() as steLastUpdated

,cast(n.schyear as char(4))+'-'+ cast(n.Schyear+1 as char(4)) as steAcademicYearDesignator

,Cast(sch as varchar(10)) as steSchoolLocalId

,case when isnull(transcode,'')='C0RR' then 'Z'

when isnull(transcode,'')='' then 'K'

when substring(transcode,1,1)='W' then 'W'

when substring(transcode,1,1)='M' then 'M'

when substring(transcode,1,1)='D' then 'D'

when substring(transcode,1,1)='C' then 

case when isnull(t.studentno,'') &lt;&gt;'' then 'T' else 'C' end

when substring(transcode,1,1)='A' then 'A'

when substring(transcode,1,1)='X' then 'X'

when substring(transcode,1,1)='S' then 'N'

END AS steEligibility

,'BPS' as steProvider
,1  as SteIscurrent

from MYBPSSQL1.bpsstu.dbo.nonbpscsm n

JOIN MYBPSSQL1.MyBPS.dbo.MyBPSConfig c

ON n.SchYear = c.Text AND c.[Key] = 'CurrentSchoolYear'

left join MYBPSSQL1.bpsstu.dbo.tpasstracking t 
on t.studentno=n.studentno and t.TpassLocation= n.sch
and isnull(t.DeactivateCode,'')='' and t.schyear=n.schyear</property>
