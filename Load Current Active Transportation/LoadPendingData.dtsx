                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT StudentNo,
       VersatransDataPending.Sch,
       ProgID,
	   BusID,
	   StopDescription,
	   StopTime,
	  convert(char(10),Cast(CASE ProgID
           WHEN 10 THEN
               'C'
           WHEN 11 THEN
               'C'
           WHEN 12 THEN
               'C'
           WHEN 20 THEN
               'D'
           WHEN 21 THEN
               'D'
           WHEN 30 THEN
               'M'
           WHEN 31 THEN
               'M'
           WHEN 32 THEN
               'M'
           WHEN 40 THEN
               'W'
           WHEN 41 THEN
               'W'
           WHEN 42 THEN
               'W'
           WHEN 50 THEN
               'T'
           WHEN 51 THEN
               'O'
       END as varchar(10))) steEligibility,
       REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(Days, 'M', 'Mo'), 'T', 'Tu'), 'W', 'We'), 'H', 'Th'), 'F', 'Fr') Days,
       CAST(EffectiveDate AS DATE) EffectiveDate,
       CASE
           WHEN InTrip IS NOT NULL THEN
               'InBound'
           WHEN OutTrip IS NOT NULL THEN
               'OutBound'
           ELSE
               'Unknown'
       END Trip,
       BusID InBusID,
       CASE
           WHEN InTrip IS NOT NULL THEN
               StopDescription
           ELSE
               schname_23
       END InStopDescription,
       CASE
           WHEN InTrip IS NOT NULL THEN
               CAST(StopTime AS TIME)
           ELSE
               vdldropofftime
       END InStopTime,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdpeVersatransX
           ELSE
               X
       END inX,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdpeVersatransY
           ELSE
               Y
       END inY,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdpeVersatransLatitude
           ELSE
               Latitude
       END inLatitude,
       CASE
           WHEN InTrip IS NOT NULL THEN
               vdpeVersatransLongitude
           ELSE
               Longitude
       END inLongitude,
       BusID OutBusID,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               StopDescription
           ELSE
               schname_23
       END OutStopDescription,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               CAST(StopTime AS TIME)
           ELSE
               vdldropofftime
       END OutStopTime,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdpeVersatransX
           ELSE
               X
       END outX,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdpeVersatransY
           ELSE
               Y
       END outY,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdpeVersatransLatitude
           ELSE
               Latitude
       END outLatitude,
       CASE
           WHEN OutTrip IS NOT NULL THEN
               vdpeVersatransLongitude
           ELSE
               Longitude
       END outLongitude
FROM Transportation.VersatransDataPending
LEFT JOIN dbo.schoollocation ON schoollocation.sch = VersatransDataPending.Sch
WHERE NOT ProgID IN ( 50, 61, 62, 70, 60 )
      AND VersatransDataPending.Sch IN (
                     SELECT DISTINCT
                         SKL_SCHOOL_ID COLLATE DATABASE_DEFAULT
                     FROM ASPENDB.x2data.dbo.STUDENT
                         JOIN ASPENDB.x2data.dbo.SCHOOL
                             ON SKL_OID = STD_SKL_OID
                     WHERE STD_ENROLLMENT_STATUS IN ( 'Active', 'Registered' )
  )
   
  ORDER BY StudentNo,Sch,steEligibility,Days,Trip</property>
