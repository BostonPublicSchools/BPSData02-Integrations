                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  steId,
       steStudentLocalId ,
       steSchoolLocalId ,
      steEligibility,
       strId,
       strSourceId,
       CASE WHEN strisCurrent IS NULL THEN 1 ELSE 0 END AS strisCurrent,
       CONVERT(VARCHAR(7),strDayOfWeek) strDayOfWeek ,
       strEffectiveDate,
       CONVERT(Varchar(8),strRoutingdirection) strRoutingdirection,
       strOriginPointVehicleNumber,
       strOriginPointBusStopDescription,
       strOriginPointArrivalTime,
       strOriginCartesianXCoordinate,
       strOriginCartesianYCoordinate,
       strOriginLatitude,
       strOriginLongitude,
       strDestinationPointVehicleNumber,
       strDestinationPointBusStopDescription,
       strDestinationPointArrivalTime,
       strDestinationCartesianXCoordinate,
       strDestinationCartesianYCoordinate,
       strDestinationLatitude,
       strDestinationLongitude
FROM Transportation.StudentTransportationEligibility
    left JOIN Transportation.StudentTransportationRouting
        ON StudentTransportationRouting.strSteId = StudentTransportationEligibility.steId
-- WHERE NULLIF(steEligiblityEndDate, '') IS NULL;
WHERE steAcademicYearDesignator =CASE WHEN MONTH(GETDATE()) &gt;= 1 AND MONTH(GETDATE()) &lt; 6 then CONCAT(YEAR(GETDATE())-1,'-', (YEAR(GETDATE())-1)+1)
                                 ELSE CONCAT(YEAR(GETDATE()),'-', (YEAR(GETDATE()))+1) END 
				 ORDER BY steStudentLocalId,steSchoolLocalId,steEligibility,strDayOfWeek,strRoutingdirection</property>
