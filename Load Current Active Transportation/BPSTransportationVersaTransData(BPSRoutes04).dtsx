                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  s.VisibleID AS StudentNo ,
        CASE WHEN LEN(CAST(BuildingCode AS VARCHAR(4))) = 4
             THEN CAST(BuildingCode AS VARCHAR(4))
             WHEN LEN(CAST(BuildingCode AS VARCHAR(4))) = 3
             THEN '0' + CAST(BuildingCode AS VARCHAR(4))
             WHEN LEN(CAST(BuildingCode AS VARCHAR(4))) = 2
             THEN '00' + CAST(BuildingCode AS VARCHAR(4))
             ELSE '000' + CAST(BuildingCode AS VARCHAR(4))
        END AS Sch ,
        prog.ProgramCode AS ProgID ,
        LEFT(vehicles_IN.vehicle, 5) AS BusID ,
        CASE WHEN prog.ProgramCode = '51' THEN 'Combo'
             WHEN prog.ProgramCode IN ( '10', '11', '12' ) THEN 'Corner'
             WHEN prog.ProgramCode IN ( '20', '21', '22', '30', '31', '32',
                                        '40', '41', '42' ) THEN 'Door'
             ELSE NULL
        END AS StopType ,
        CASE WHEN ISNULL(p_IN.CustomDescription, '') &lt;&gt; ''
             THEN p_IN.CustomDescription
             ELSE p_IN.Description
        END AS StopDescription ,
        REPLACE(REPLACE(LTRIM(RTRIM(CASE WHEN CHARINDEX(':',
                                                        LTRIM(SUBSTRING(CAST(times_IN.PickupTime AS VARCHAR),
                                                              12,
                                                              LEN(GETDATE())))) = 2
                                         THEN '0'
                                              + LTRIM(SUBSTRING(CAST(times_IN.PickupTime AS VARCHAR),
                                                              12,
                                                              LEN(GETDATE())))
                                         ELSE SUBSTRING(CAST(times_IN.PickupTime AS VARCHAR),
                                                        12, LEN(GETDATE()))
                                    END)), 'AM', ' AM'), 'PM', ' PM') AS StopTime ,
        route_IN.Days ,
        ( CASE WHEN ISDATE(siEffDate.Description) = 1
               THEN CAST(siEffDate.Description AS DATETIME)
               ELSE GETDATE()
          END ) AS EffectiveDate ,
        route_IN.VisibleRouteID AS InTrip ,
        NULL AS OutTrip ,
        SUBSTRING(route_IN.VisibleRouteID, 5, 4) AS RouteTrip ,
        SUBSTRING(siMonitor.Description, 1, 1) AS Monitor ,
        SUBSTRING(ISNULL(siGeoCode.Description, 999), 1, 3) AS homegeo ,
        pu.HouseNumber AS StreetNo ,
        pu.Street AS Street ,
        pu.ZipCode AS Zip ,
        GETDATE() AS LastUpdateTime ,
        p_IN.X ,
        p_IN.Y ,
        p_IN.Latitude ,
        p_IN.Longitude
FROM    dbo.Student s WITH ( NOLOCK )
        LEFT OUTER JOIN dbo.StudentAddress pu WITH ( NOLOCK ) ON pu.StudentID = s.RecordID
                                                              AND pu.AddressType = 'P'
        LEFT JOIN dbo.Building b WITH ( NOLOCK ) ON b.RecordID = s.SchoolID
        LEFT JOIN dbo.Program prog WITH ( NOLOCK ) ON prog.RecordID = s.ProgramID
        LEFT JOIN dbo.StudentInfoFields siMonitor WITH ( NOLOCK ) ON siMonitor.StudentID = s.RecordID
                                                              AND siMonitor.InfoID = 1
        LEFT JOIN dbo.StudentInfoFields siGeoCode WITH ( NOLOCK ) ON siGeoCode.StudentID = s.RecordID
                                                              AND siGeoCode.InfoID = 9
        LEFT JOIN dbo.StudentInfoFields siEffDate WITH ( NOLOCK ) ON siEffDate.StudentID = s.RecordID
                                                              AND siEffDate.InfoID = 12
        LEFT JOIN dbo.StudentInfoFields siNonAmb WITH ( NOLOCK ) ON siNonAmb.StudentID = s.RecordID
                                                              AND siNonAmb.InfoID = 15
        LEFT JOIN dbo.TransportationRequest tr_IN WITH ( NOLOCK ) ON tr_IN.StudentID = s.RecordID
                                                              AND tr_IN.RequestType = 'I'
        LEFT JOIN dbo.TransportationSatisfaction ts_IN WITH ( NOLOCK ) ON tr_IN.RecordID = ts_IN.TRexID
        LEFT JOIN dbo.RouteDetail rd_IN WITH ( NOLOCK ) ON ts_IN.PickupRouteDetailID = rd_IN.RecordID
        LEFT JOIN dbo.Route route_IN WITH ( NOLOCK ) ON rd_IN.RouteID = route_IN.RecordID
        LEFT JOIN dbo.Point p_IN WITH ( NOLOCK ) ON p_IN.RecordID = tr_IN.OriginPointID
        LEFT JOIN dbo.v_BusRideTimes times_IN WITH ( NOLOCK ) ON times_IN.TRexID = ts_IN.TRexID
                                                              AND times_IN.Days = ts_IN.Days
        LEFT JOIN dbo.Vehicles vehicles_IN WITH ( NOLOCK ) ON vehicles_IN.RecordID = route_IN.VehicleID
WHERE   LEN(VisibleID) = 6
        AND ISNULL(ProgramCode, '') &lt;&gt; '70'
UNION
SELECT  s.VisibleID AS 'StudentNo' ,
        ( CASE WHEN LEN(CAST(BuildingCode AS VARCHAR(4))) = 4
               THEN CAST(BuildingCode AS VARCHAR(4))
               WHEN LEN(CAST(BuildingCode AS VARCHAR(4))) = 3
               THEN '0' + CAST(BuildingCode AS VARCHAR(4))
               WHEN LEN(CAST(BuildingCode AS VARCHAR(4))) = 2
               THEN '00' + CAST(BuildingCode AS VARCHAR(4))
               ELSE '000' + CAST(BuildingCode AS VARCHAR(4))
          END ) AS 'Sch' ,
        prog.ProgramCode AS 'ProgID' ,
        LEFT(vehicles_OUT.vehicle, 5) AS 'BusID' ,
        CASE WHEN prog.ProgramCode = '51' THEN 'Combo'
             WHEN prog.ProgramCode IN ( '10', '11', '12' ) THEN 'Corner'
             WHEN prog.ProgramCode IN ( '20', '21', '22', '30', '31', '32',
                                        '40', '41', '42' ) THEN 'Door'
             ELSE ''
        END AS StopType ,
        CASE WHEN ISNULL(p_OUT.CustomDescription, '') &lt;&gt; ''
             THEN p_OUT.CustomDescription
             ELSE p_OUT.Description
        END AS 'StopDescription' ,
        REPLACE(REPLACE(LTRIM(RTRIM(CASE WHEN CHARINDEX(':',
                                                        LTRIM(SUBSTRING(CAST(times_OUT.DropoffTime AS VARCHAR),
                                                              12,
                                                              LEN(GETDATE())))) = 2
                                         THEN '0'
                                              + LTRIM(SUBSTRING(CAST(times_OUT.DropoffTime AS VARCHAR),
                                                              12,
                                                              LEN(GETDATE())))
                                         ELSE SUBSTRING(CAST(times_OUT.DropoffTime AS VARCHAR),
                                                        12, LEN(GETDATE()))
                                    END)), 'AM', ' AM'), 'PM', ' PM') AS 'StopTime' ,
        route_OUT.Days ,
        ( CASE WHEN ISDATE(siEffDate.Description) = 1
               THEN CAST(siEffDate.Description AS DATETIME)
               ELSE GETDATE()
          END ) AS 'EffectiveDate' ,
        NULL AS 'InTrip' ,
        route_OUT.VisibleRouteID AS OutTrip ,
        SUBSTRING(route_OUT.VisibleRouteID, 5, 4) AS RouteTrip ,
        SUBSTRING(ISNULL(siMonitor.Description, ''), 1, 1) AS Monitor ,
        SUBSTRING(ISNULL(siGeoCode.Description, 999), 1, 3) AS HomeGeo ,
        d.HouseNumber AS StreetNo ,
        d.Street AS Street ,
        d.ZipCode AS Zip ,
        GETDATE() AS LastUpdateTime ,
        p_OUT.X ,
        p_OUT.Y ,
        p_OUT.Latitude ,
        p_OUT.Longitude
FROM    dbo.Student s WITH ( NOLOCK )
        LEFT JOIN dbo.Program prog WITH ( NOLOCK ) ON prog.RecordID = s.ProgramID
        LEFT OUTER JOIN dbo.StudentAddress d WITH ( NOLOCK ) ON d.StudentID = s.RecordID
                                                              AND d.AddressType = 'D'
        LEFT JOIN dbo.StudentInfoFields siGeoCode WITH ( NOLOCK ) ON siGeoCode.StudentID = s.RecordID
                                                              AND siGeoCode.InfoID = 9
        LEFT JOIN dbo.StudentInfoFields siEffDate WITH ( NOLOCK ) ON siEffDate.StudentID = s.RecordID
                                                              AND siEffDate.InfoID = 12
        LEFT JOIN dbo.StudentInfoFields siMonitor WITH ( NOLOCK ) ON siMonitor.StudentID = s.RecordID
                                                              AND siMonitor.InfoID = 1
        LEFT JOIN dbo.Building b WITH ( NOLOCK ) ON b.RecordID = s.SchoolID
        LEFT JOIN dbo.TransportationRequest tr_OUT WITH ( NOLOCK ) ON tr_OUT.StudentID = s.RecordID
                                                              AND tr_OUT.RequestType = 'O'
        LEFT JOIN dbo.TransportationSatisfaction ts_OUT WITH ( NOLOCK ) ON tr_OUT.RecordID = ts_OUT.TRexID
        LEFT JOIN dbo.RouteDetail rd_OUT WITH ( NOLOCK ) ON ts_OUT.DropoffRouteDetailID = rd_OUT.RecordID
        LEFT JOIN dbo.Route route_OUT WITH ( NOLOCK ) ON rd_OUT.RouteID = route_OUT.RecordID
        LEFT JOIN dbo.Point p_OUT WITH ( NOLOCK ) ON p_OUT.RecordID = tr_OUT.DestinationPointID
        LEFT JOIN dbo.v_BusRideTimes times_OUT WITH ( NOLOCK ) ON times_OUT.TRexID = ts_OUT.TRexID
                                                              AND times_OUT.Days = ts_OUT.Days
        LEFT JOIN dbo.Vehicles vehicles_OUT WITH ( NOLOCK ) ON vehicles_OUT.RecordID = route_OUT.VehicleID
WHERE   LEN(VisibleID) = 6
        AND ISNULL(ProgramCode, '') &lt;&gt; '70';</property>
